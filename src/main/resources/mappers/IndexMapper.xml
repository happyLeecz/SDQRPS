<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.gxu.sdqrps.dao.IndexMapper">
    <select id="getByIndexName" resultType="Index">
     select * from indexes where index_description = #{indexDescription}
    </select>

    <select id="getByIndexId" resultType="Index">
     select * from indexes where index_id = #{indexId}
    </select>
</mapper>